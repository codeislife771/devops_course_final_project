{
  "id": null,
  "uid": "blackbox-starter",
  "title": "Blackbox Starter",
  "tags": ["blackbox", "starter"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "panels": [
    {
      "type": "timeseries",
      "title": "Probe Success",
      "id": 1,
      "targets": [{ "refId": "A", "expr": "probe_success{job=\"blackbox_http\"}" }],
      "fieldConfig": { "defaults": { "unit": "bool", "min": 0, "max": 1 }, "overrides": [] },
      "options": { "legend": { "showLegend": true } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Probe Duration (s)",
      "id": 2,
      "targets": [{ "refId": "A", "expr": "probe_duration_seconds{job=\"blackbox_http\"}" }],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "showLegend": true } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "type": "stat",
      "title": "HTTP Status Code (last)",
      "id": 3,
      "targets": [{ "refId": "A", "expr": "max_over_time(probe_http_status_code{job=\"blackbox_http\"}[5m])" }],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 200 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value"
      },
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 8 }
    }
  ],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" }
}
